{
  "presets": [
    ["@babel/env", {
      "modules": false,
      "useBuiltIns": "usage",
    }]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-regenerator",
    ["@babel/plugin-transform-runtime",
     {
        "helpers": true,
        "regenerator": true
     }
    ]
  ]
}

